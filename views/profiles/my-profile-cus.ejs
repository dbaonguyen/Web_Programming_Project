<%- include('../partials/header.ejs') %>
    <!-- header end -->
    <div class="breadcrumb-area pt-205 pb-210 theme-bg-2">
        <div class="container">
            <div class="breadcrumb-content text-center">
                <h2>My profile</h2>

                <img src="<%= photo %>" alt="user avatar" width="300" height="300">

            </div>
        </div>
    </div>



    <div class="choose-area pb-100">
        <div class="container">

            <div class="row">
                <div class="col-lg-2 col-md-12 theme-bg-1">

                </div>
                <div class="col-lg-8 col-md-12">
                    <div class="all-causes">
                        <div class="row">
                            <div class="col-md-12 causes-res">
                                <div class="choose-wrapper">
                                    <h4>Name:</h4>
                                    <p>
                                        <%= name %>
                                    </p>
                                </div>
                                <div class="choose-wrapper">
                                    <h4>Phone number:</h4>
                                    <p>
                                        <%= phone %>
                                    </p>
                                </div>
                                <div class="choose-wrapper">
                                    <h4>Email:</h4>
                                    <p>
                                        <%= email %>
                                    </p>
                                </div>
                                <div class="choose-wrapper">
                                    <h4>Addres:</h4>
                                    <p>
                                        <%= address %>
                                    </p>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
                <div class="col-lg-2 col-md-12 theme-bg-1">

                </div>
            </div>
        </div>
    </div>


    <%- include ('../partials/footer') %>