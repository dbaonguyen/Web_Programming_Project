<%- include ('partials/header') %>
		<div class="breadcrumb-area pt-205 pb-210" style="background-image: url(img/bg/breadcrumb.jpg)">
            <div class="container">
                <div class="breadcrumb-content text-center">
                    <h2>login</h2>
                    <ul>
                        <li><a href="#">home</a></li>
                        <li> login </li>
                    </ul>
                </div>
            </div>
        </div>
        <!-- login-area start -->
        <div class="register-area ptb-100">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md-12 col-12 col-lg-6 col-xl-6 ml-auto mr-auto">
                        <div class="login-form-container">
                            <div class="login-form">
                                <div class="col-4 role-choice choosen">
                                    <a href="">Customer</a>
                                </div>
                                <div class="col-4 role-choice">
                                    <a href="">Vendor</a>
                                </div>
                                <div class="col-4 role-choice">
                                    <a href="">Shipper</a>
                                </div>
                            </div>
                            
                        </div>
                        <div class="login">
                            <div class="login-form-container">
                                <div class="login-form">
                                    <form action="#" method="post">
                                        <input type="text" name="user-name" placeholder="Username">
                                        <input type="password" name="user-password" placeholder="Password">
                                        <div class="button-box">
                                            <div class="row login-toggle-btn">
                                                <input class="col-1" type="checkbox">
                                                <label class="col-11">Remember me</label>
                                            </div>
                                            <button type="submit" class="default-btn floatright">Login</button>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- login-area end -->
